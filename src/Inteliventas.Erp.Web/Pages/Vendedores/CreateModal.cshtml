@page
@using Inteliventas.Erp.Localization
@using Inteliventas.Erp.Web.Pages.Vendedores
@using Microsoft.Extensions.Localization
@using Volo.Abp.AspNetCore.Mvc.UI.Bootstrap.TagHelpers.Modal
@model CreateModalModel
@inject IStringLocalizer<ErpResource> L
@{
    Layout = null;
}
<abp-dynamic-form abp-model="Vendedor" asp-page="/Vendedores/CreateModal">
    <abp-modal>
        <abp-modal-header title="@L["NewVendor"].Value"></abp-modal-header>
        <abp-modal-body>
            <abp-form-content />
        </abp-modal-body>
        <abp-modal-footer buttons="@(AbpModalButtons.Cancel|AbpModalButtons.Save)"></abp-modal-footer>
    </abp-modal>
</abp-dynamic-form>